{
  "name": "wesell/ultra-import",
  "description": "Laravel package for importing Ultra B2B catalog and exporting XML feeds.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "WeSellUltra\\Import\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WeSellUltra\\Import\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WeSellUltra\\Import\\Providers\\UltraImportServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "composer validate",
      "@php vendor/bin/phpunit"
    ]
  }
}
